            </Image.RenderTransform>
        </Image>
        <Grid x:Name="grid" Grid.Row="1" Grid.RowSpan="4" Background="#3d9ef0" Margin="10,30,10,50" Opacity="0.1" RenderTransformOrigin="0.5,0.5">

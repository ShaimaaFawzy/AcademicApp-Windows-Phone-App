        	</Grid.RenderTransform>
        </Grid>
        <StackPanel x:Name="stackPanel" Width="200" Grid.Row="0" Grid.RowSpan="2"  HorizontalAlignment="Left"  VerticalAlignment="Center" Background="#53b2f6" RenderTransformOrigin="0.5,0.5">

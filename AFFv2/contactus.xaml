        	</StackPanel.RenderTransform>
            <TextBlock Foreground="White" Text="Contact us" HorizontalAlignment="Center" FontSize="25" FontFamily="Segoe UI" FontWeight="Light" VerticalAlignment="Center"/>
        </StackPanel>

        <Image x:Name="image" Tap="image_Tap" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="0,0,121,0" Source="/Assets/contact us/fb1.png"  Height="100" RenderTransformOrigin="0.5,0.5" >
